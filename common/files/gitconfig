[core]
editor = vim

[color]
branch=auto
diff=auto
interactive=auto
status=auto

[alias]
br = branch
ci = commit
cl = clone
co = checkout
cp = cherry-pick
dc = diff --cached
diff = diff --word-diff
lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)
st = status
